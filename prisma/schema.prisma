// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model City {
  id String @default(cuid()) @id
  city_name String
  state_abbr String
  metric_name String
  group_name String
  est Float
  state String
  population Int
  lat Float
  lon Float
}